<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
  <class name="sim.datatransfer.CourseTypeDTO" table="course_type">
    <meta attribute="class-description"> Course Type Details </meta>
    <id name="typeId" type="int" column="type_id">
      <generator class="native" />
    </id>
    <property name="typeName" column="course_type_name" />
  </class>
</hibernate-mapping>
